<!DOCTYPE html>
<html>
  <head>
    <title>Smart Home Users</title>
    <style>
      body {
        font-family: Arial, sans-serif;
        margin: 2rem;
      }
      table {
        border-collapse: collapse;
        width: 80%;
      }
      th,
      td {
        padding: 10px;
        border: 1px solid #ccc;
        text-align: left;
      }
      th {
        background-color: #f2f2f2;
      }
    </style>
  </head>
  <body>
    <h1>Registered Users</h1>
    <table>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Contact</th>
        <th>Address</th>
      </tr>
      <% users.forEach(user => { %>
      <tr>
        <td><%= user.UserID %></td>
        <td><%= user.Name %></td>
        <td><%= user.Email %></td>
        <td><%= user.ContactNumber %></td>
        <td><%= user.Address %></td>
      </tr>
      <% }); %>
    </table>
  </body>
</html>
